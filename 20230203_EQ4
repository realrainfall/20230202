# Q4 Answer Template

def solution(arr):
    answer = 1
    num = []
    for j in arr:
        for i in range(1, 101):
            if j % i == 0:
                if answer % i != 0:
                    answer *= i
    return answer

arr = [None]*15
arr = [2,6,8,14]
answer = solution(arr)
print(answer)

arr = [None]*15
arr = [1,2,3]
answer = solution(arr)
print(answer)
